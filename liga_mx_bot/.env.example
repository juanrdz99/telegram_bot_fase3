# LiveScore API credentials
# Sign up at https://livescore-api.com/ to get your API key
LIVESCORE_API_KEY=your_livescore_api_key_here

# Telegram Bot credentials
# Create a bot using @BotFather on Telegram to get your token
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Telegram Chat ID
# This is the ID of the group or channel where you want to send notifications
# You can get this by adding @RawDataBot to your group or using the getUpdates API method
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
